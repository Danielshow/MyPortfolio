<% if flash[:alert] %>
  <%= js add_gritter(flash[:alert], :title => "Daniel Shotonwa Portfolio") %>
<% elsif flash[:error] %>
  <%= js add_gritter(flash[:error], :title => "Daniel Shotonwa Portfolio") %>
<% else %>
  <%= js add_gritter(flash[:notice], :title => "Daniel Shotonwa Portfolio") %>
<% end %>
