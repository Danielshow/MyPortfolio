<div class="abou-page">
  <h4 class=" text-white text-center">Tech News</h4>
  <div class="row">
   <% @technews.each do |tweet| %>
     <div class="col-md-6 tweetcard mt-2 mb-2">
        <div class="card tweet">
          <%= twitter_parser(tweet) %>
        </div>
      </div>
    <% end %>
  </div>
</div>
